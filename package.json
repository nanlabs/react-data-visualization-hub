    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "build-css": "tailwindcss build styles.css -o output.css"
  },
  "version": "0.1.0",
  "description": "A Web App created by create-awesome-node-app",
  "engines": {
